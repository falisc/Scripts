wait(1.2)
game.StarterGui:SetCore("SendNotification", {
Title = "Egg Sniper.lua";
Text = "Scripter: such#9999";
Icon = "rbxassetid://10638813518";
Duration = 5;
})

local players = game:GetService('Players')
local lp = players.LocalPlayer

local function getchar()
    return lp.Character or lp.CharacterAdded:Wait()
end

getchar():WaitForChild('FULLY_LOADED_CHAR')

if AntiAfk then
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
 end

local function get_eggs()
    local eggs = {}

    for i,v in next, workspace:WaitForChild("Ignored"):GetChildren() do
        if v.Name:match('^Egg') then
            table.insert(eggs, v)
        end
    end

    return eggs
end

local function collect_eggs()
    for i,v in next, get_eggs() do
        firetouchinterest(getchar():WaitForChild('HumanoidRootPart'), v, 0)
        firetouchinterest(getchar():WaitForChild('HumanoidRootPart'), v, 1)
    end
end

while getgenv().EggFarm do
    collect_eggs()
    task.wait()
end
